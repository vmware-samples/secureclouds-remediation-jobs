{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3EnableAccessLogging",
            "Effect": "Allow",
            "Action": [
                "s3:PutEncryptionConfiguration",
                "s3:PutBucketLogging",
                "s3:PutBucketAcl",
                "s3:GetBucketAcl",
                "s3:CreateBucket",
                "s3:ListBucket"
            ],
            "Resource": "*"
        }
    ]
}
